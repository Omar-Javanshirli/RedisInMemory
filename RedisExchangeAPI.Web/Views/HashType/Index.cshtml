@model Dictionary<string,string>

@{
    ViewData["Title"] = "Index";
}


<a asp-action="DeleteFirstItem">Baştan sil</a>

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="text" name="value" />
    <button type="submit">Save</button>
</form>

@foreach (var item in Model)
{
    <h2>
        @item.Key -> @item.Value
        <a asp-controller="HashType" asp-action="Remove" asp-route-name="@item.Key">sil</a>
    </h2>
}